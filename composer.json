{
    "name": "yish/generators",
    "description": "This package extends the core file generators that are included with Laravel 5",
    "keywords": ["laravel", "patterns", "generator"],
    "license": "MIT",
    "homepage": "https://github.com/Mombuyish/Laravel-Oh-Generators",
    "authors": [
        {
            "name": "Yish",
            "email": "mombuartworks@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "Yish\\Generators\\": "src/"
        },
        "files": [
            "src/Foundation/helpers.php"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "Yish\\Generators\\Tests\\": "tests/"
        }
    },

    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "5.*",
        "illuminate/support": "5.*",
        "illuminate/view": "5.*"
	},
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "^3.5",
        "orchestra/database": "^3.5"
    },

    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Yish\\Generators\\GeneratorsServiceProvider"
            ]
        }
    },

    "minimum-stability": "stable"
}
